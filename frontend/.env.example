# .env.example - Template for environment variables
# Copy this file to .env and update with your actual values

# Builder.io Configuration (if using Builder.io)
VITE_PUBLIC_BUILDER_KEY=your_builder_io_api_key_here

# Test/Development Configuration
PING_MESSAGE="your_message_here"

# Firebase Configuration (add your actual values)
VITE_FIREBASE_API_KEY=your_firebase_api_key
VITE_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id

# LLM Configuration (if using external LLM)
VITE_LLM_API_URL=https://your-api-endpoint.com
VITE_LLM_API_KEY=your_llm_api_key
VITE_LLM_MODEL=your_model_name
